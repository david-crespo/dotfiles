{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "env": {
    "CLAUDE_CODE_DISABLE_TERMINAL_TITLE": "1"
  },
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash(cat:*)",
      "Bash(echo:*)",
      "Bash(grep:*)",
      "Bash(rg:*)",
      "Bash(find:*)",
      "Bash(fd:*)",
      "Bash(tree:*)",
      "Bash(tokei:*)",
      "Bash(awk:*)",
      "Bash(head:*)",
      "Bash(tail:*)",
      "Bash(wc:*)",
      "Bash(sort:*)",
      "Bash(uniq:*)",
      "Bash(diff:*)",
      "Bash(git log:*)",
      "Bash(git diff:*)",
      "Bash(git status:*)",
      "Bash(git show:*)",
      "Bash(jj log:*)",
      "Bash(jj diff:*)",
      "Bash(jj status:*)",
      "Bash(jj show:*)",
      "Bash(jj file show:*)",
      "Bash(jj new:*)",
      "Bash(jj desc:*)",
      "Bash(jj commit:*)",
      "Bash(cargo check:*)",
      "Bash(cargo test:*)",
      "Bash(cargo t:*)",
      "Bash(cargo nextest run:*)",
      "Bash(cargo tree:*)",
      "Bash(cargo doc:*)",
      "Bash(cargo fmt:*)",
      "Bash(cargo clippy:*)",
      "Bash(npm test:*)",
      "Bash(npm run tsc:*)",
      "Bash(npm run typecheck:*)",
      "Bash(npm run lint:*)",
      "Bash(npm run check:*)",
      "Bash(npm ls:*)",
      "Bash(npm info:*)",
      "Bash(npm outdated:*)",
      "Bash(npx tsc:*)",
      "Bash(npx vitest:*)",
      "Bash(npx tsx:*)",
      "Bash(deno check:*)",
      "Bash(deno fmt:*)",
      "Bash(deno lint:*)",
      "Bash(deno test:*)",
      "Bash(deno info:*)",
      "Bash(go doc:*)",
      "Bash(gh issue view:*)",
      "Bash(gh pr view:*)",
      "Bash(gh pr list:*)",
      "Bash(gh release view:*)",
      "Bash(sw_vers:*)",
      "Bash(which:*)",
      "Bash(type:*)",
      "Bash(man:*)",
      "Bash(tldr:*)",
      "Bash(aipr tracking:*)",
      "Bash(aipr discussion:*)",
      "WebSearch",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:raw.githubusercontent.com)",
      "WebFetch(domain:docs.rs)",
      "WebFetch(domain:stackoverflow.com)",
      "WebFetch(domain:docs.oxide.computer)",
      "WebFetch(domain:vite.dev)",
      "WebFetch(domain:shiki.style)",
      "WebFetch(domain:docs.jj-vcs.dev)",
      "WebFetch(domain:oxc.rs)"
    ],
    "ask": [
      "Bash(jj restore:*)",
      "Bash(jj squash:*)",
      "Bash(jj rebase:*)"
    ]
  },
  "model": "opus",
  "statusLine": {
    "type": "command",
    "command": "~/.claude/statusline.ts"
  },
  "enabledPlugins": {
    "ast-grep@ast-grep-marketplace": true,
    "typescript-lsp@claude-plugins-official": true,
    "rust-analyzer-lsp@claude-plugins-official": false,
    "ralph-wiggum@claude-plugins-official": true
  },
  "alwaysThinkingEnabled": true,
  "gitAttribution": false,
  "feedbackSurveyState": {
    "lastShownTime": 1754073691732
  }
}
